#, no-c-format
msgid "POINT(0 0 0) -- XYZ"
msgstr ""

#. Tag: para
#, no-c-format
msgid "SRID=32632;POINT(0 0) -- XY with SRID"
msgstr ""

#. Tag: para
#, no-c-format
msgid "POINTM(0 0 0) -- XYM"
msgstr ""

#. Tag: para
#, no-c-format
msgid "POINT(0 0 0 0) -- XYZM"
msgstr ""

#. Tag: para
#, no-c-format
