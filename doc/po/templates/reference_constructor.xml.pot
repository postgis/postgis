"\n"
" st_astext\n"
"------------------------------\n"
" POLYGON((-10018754.1713945 0,-10018754.1713945 10018754.1713945,0 "
"10018754.1713945,0 0,-10018754.1713945 0))\n"
"\n"
"SELECT ST_AsText( ST_TileEnvelope(3, 1, 1, ST_MakeEnvelope(-180, -90, 180, "
"90, 4326) ) );\n"
"\n"
"                      st_astext\n"
"------------------------------------------------------\n"
