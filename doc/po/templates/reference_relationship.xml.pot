#. Tag: refname
#: reference_relationship.xml:940
#, no-c-format
msgid "ST_OrderingEquals"
msgstr ""

#. Tag: refpurpose
#: reference_relationship.xml:942
#, no-c-format
msgid ""
"Returns true if two geometries represent the same geometry and have points "
"in the same directional order."
msgstr ""

#. Tag: funcprototype
#: reference_relationship.xml:948
#, no-c-format
msgid ""
"<funcdef>boolean <function>ST_OrderingEquals</function></funcdef> "
"<paramdef><type>geometry </type> <parameter>A</parameter></paramdef> "
"<paramdef><type>geometry </type> <parameter>B</parameter></paramdef>"
