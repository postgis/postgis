msgid ""
"--3d endpoint\n"
"SELECT ST_AsEWKT(ST_EndPoint('LINESTRING(1 1 2, 1 2 3, 0 0 5)'));\n"
"  st_asewkt\n"
"--------------\n"
" POINT(0 0 5)"
msgstr ""

#. Tag: para
#: reference_accessor.xml:457
#, no-c-format
msgid "End point of a CircularString"
msgstr ""

#. Tag: programlisting
