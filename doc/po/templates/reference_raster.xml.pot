      "SELECT ST_AsGDALRaster(rast, 'JPEG') As rastjpg\n"
      "FROM dummy_rast WHERE rid=1;\n"
      "\n"
      "SELECT ST_AsGDALRaster(rast, 'JPEG', ARRAY!['QUALITY=50']) As rastjpg\n"
      "FROM dummy_rast WHERE rid=2;"
