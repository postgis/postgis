      "    2 |     3 | f\n"
      "    2 |     1 | f\n"
      "    3 |     2 | t\n"
      "    3 |     3 | t\n"
      "    3 |     1 | f\n"
      "    1 |     2 | t\n"
      "    1 |     3 | t\n"
      "    1 |     1 | t"
