msgstr ""

#. Tag: programlisting
#: reference_raster.xml:238
#, no-c-format
msgid "SELECT ROW(2, '0-100:1-10, 101-500:11-150,501 - 10000: 151-254', '8BUI', 255)::reclassarg;"
msgstr ""

#. Tag: title
#: reference_raster.xml:241
#, no-c-format
