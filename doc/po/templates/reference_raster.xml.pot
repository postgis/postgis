      "                                                                '8BUI'::text, 1, 0\n"
      "                                                        ),\n"
      "                                                        1, 1, 0.\n"
      "                                                ),\n"
      "                                                2, 3, 0.\n"
      "                                        ),\n"
      "                                        3, 5, 0.\n"
      "                                ),\n"
      "                                4, 2, 0.\n"
      "                        ),\n"
      "                        5, 4, 0.\n"
      "                ) AS rast\n"
