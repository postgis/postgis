#. Tag: refname
#: reference_editor.xml:687
#, no-c-format
msgid "ST_ForceRHR"
msgstr ""

#. Tag: refpurpose
#: reference_editor.xml:689
#, no-c-format
msgid ""
"Force the orientation of the vertices in a polygon to follow the Right-Hand-"
"Rule."
msgstr ""

#. Tag: funcprototype
#: reference_editor.xml:695
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_ForceRHR</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>g</parameter></paramdef>"
msgstr ""

#. Tag: para
#: reference_editor.xml:706
#, no-c-format
msgid ""
"Forces the orientation of the vertices in a polygon to follow a Right-Hand-"
"Rule, in which the area that is bounded by the polygon is to the right of "
"the boundary. In particular, the exterior ring is orientated in a clockwise "
"direction and the interior rings in a counter-clockwise direction. This "
"function is a synonym for <xref linkend=\"ST_ForcePolygonCW\"/>"
msgstr ""

#. Tag: para
#: reference_editor.xml:713
#, no-c-format
msgid ""
"The above definition of the Right-Hand-Rule conflicts with definitions used "
"in other contexts. To avoid confusion, it is recommended to use "
"ST_ForcePolygonCW."
msgstr ""

#. Tag: programlisting
#: reference_editor.xml:726
#, no-c-format
msgid ""
"SELECT ST_AsEWKT(\n"
"  ST_ForceRHR(\n"
"        'POLYGON((0 0 2, 5 0 2, 0 5 2, 0 0 2),(1 1 2, 1 3 2, 3 1 2, 1 1 "
"2))'\n"
"  )\n"
");\n"
"                                                  st_asewkt\n"
"--------------------------------------------------------------\n"
" POLYGON((0 0 2,0 5 2,5 0 2,0 0 2),(1 1 2,3 1 2,1 3 2,1 1 2))\n"
"(1 row)"
msgstr ""

#. Tag: para
#: reference_editor.xml:732
#, no-c-format
msgid ""
", <xref linkend=\"ST_ForcePolygonCW\"/>, <xref linkend=\"ST_IsPolygonCCW\"/"
">, <xref linkend=\"ST_IsPolygonCW\"/>, <xref linkend=\"ST_BuildArea\"/>, "
"<xref linkend=\"ST_Polygonize\"/>, <xref linkend=\"ST_Reverse\"/>"
msgstr ""

#. Tag: refname
#: reference_editor.xml:745
#, no-c-format
msgid "ST_ForceCurve"
msgstr ""

#. Tag: refpurpose
#: reference_editor.xml:747
#, no-c-format
msgid "Upcast a geometry into its curved type, if applicable."
msgstr ""

#. Tag: funcprototype
#: reference_editor.xml:752
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_ForceCurve</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>g</parameter></paramdef>"
msgstr ""

#. Tag: para
#: reference_editor.xml:763
#, no-c-format
msgid ""
"Turns a geometry into its curved representation, if applicable: lines become "
"compoundcurves, multilines become multicurves polygons become curvepolygons "
"multipolygons become multisurfaces. If the geometry input is already a "
"curved representation returns back same as input."
msgstr ""

#. Tag: para
#: reference_editor.xml:769 reference_editor.xml:1144 reference_editor.xml:1651
#, no-c-format
msgid "Availability: 2.2.0"
msgstr ""

#. Tag: programlisting
#: reference_editor.xml:778
#, no-c-format
msgid ""
"SELECT ST_AsText(\n"
"  ST_ForceCurve(\n"
"        'POLYGON((0 0 2, 5 0 2, 0 5 2, 0 0 2),(1 1 2, 1 3 2, 3 1 2, 1 1 "
"2))'::geometry\n"
"  )\n"
");\n"
"                              st_astext\n"
"----------------------------------------------------------------------\n"
" CURVEPOLYGON Z ((0 0 2,5 0 2,0 5 2,0 0 2),(1 1 2,1 3 2,3 1 2,1 1 2))\n"
"(1 row)"
msgstr ""

#. Tag: refname
#: reference_editor.xml:790
#, no-c-format
msgid "ST_LineMerge"
msgstr ""

#. Tag: refpurpose
#: reference_editor.xml:792
#, no-c-format
msgid ""
"Return a (set of) LineString(s) formed by sewing together a MULTILINESTRING."
msgstr ""

#. Tag: funcprototype
#: reference_editor.xml:798
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_LineMerge</function></funcdef> "
"<paramdef><type>geometry </type> <parameter>amultilinestring</parameter></"
"paramdef>"
msgstr ""

#. Tag: para
#: reference_editor.xml:808
#, no-c-format
msgid ""
"Returns a (set of) LineString(s) formed by sewing together the constituent "
"line work of a MULTILINESTRING."
msgstr ""

#. Tag: para
#: reference_editor.xml:810
#, no-c-format
msgid ""
"Only use with MULTILINESTRING/LINESTRINGs. If you feed a polygon or geometry "
"collection into this function, it will return an empty GEOMETRYCOLLECTION"
msgstr ""

#. Tag: para
#: reference_editor.xml:812 reference_editor.xml:1536
#, no-c-format
msgid "Performed by the GEOS module."
msgstr ""

#. Tag: para
#: reference_editor.xml:814
#, no-c-format
msgid "Will strip the M dimension."
msgstr ""

#. Tag: programlisting
#: reference_editor.xml:820
#, no-c-format
msgid ""
"SELECT ST_AsText(ST_LineMerge(\n"
"ST_GeomFromText('MULTILINESTRING((-29 -27,-30 -29.7,-36 -31,-45 -33),(-45 "
"-33,-46 -32))')\n"
"                )\n"
");\n"
"st_astext\n"
"--------------------------------------------------------------------------------------------------\n"
"LINESTRING(-29 -27,-30 -29.7,-36 -31,-45 -33,-46 -32)\n"
"(1 row)\n"
"\n"
"--If can't be merged - original MULTILINESTRING is returned\n"
"SELECT ST_AsText(ST_LineMerge(\n"
"ST_GeomFromText('MULTILINESTRING((-29 -27,-30 -29.7,-36 -31,-45 -33),(-45.2 "
"-33.2,-46 -32))')\n"
")\n"
");\n"
"st_astext\n"
"----------------\n"
"MULTILINESTRING((-45.2 -33.2,-46 -32),(-29 -27,-30 -29.7,-36 -31,-45 -33))\n"
"\n"
"-- example with Z dimension\n"
"SELECT ST_AsText(ST_LineMerge(\n"
"ST_GeomFromText('MULTILINESTRING((-29 -27 11,-30 -29.7 10,-36 -31 5,-45 -33 "
"6), (-29 -27 12,-30 -29.7 5), (-45 -33 1,-46 -32 11))')\n"
"                )\n"
");\n"
"st_astext\n"
"--------------------------------------------------------------------------------------------------\n"
"LINESTRING Z (-30 -29.7 5,-29 -27 11,-30 -29.7 10,-36 -31 5,-45 -33 1,-46 "
"-32 11)\n"
