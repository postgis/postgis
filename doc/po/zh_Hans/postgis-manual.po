msgstr "8BUI - 8 位无符号整数"

#. Tag: para
#, no-c-format
msgid "16BSI - 16-bit signed integer"
msgstr "16BSI - 16 位有符号整数"

#. Tag: para
#, no-c-format
msgid "16BUI - 16-bit unsigned integer"
msgstr "16BUI - 16 位无符号整数"

#. Tag: para
#, no-c-format
msgid "32BSI - 32-bit signed integer"
msgstr "32BSI - 32 位有符号整数"

#. Tag: para
#, no-c-format
