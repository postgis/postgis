#, no-c-format
msgid ""
", <xref linkend=\"RT_ST_PixelAsPolygon\"/>, <xref "
"linkend=\"RT_ST_PixelAsPolygons\"/>, <xref linkend=\"RT_ST_PixelAsPoint\"/>, "
"<xref linkend=\"RT_ST_PixelAsPoints\"/>, <xref "
"linkend=\"RT_ST_PixelAsCentroids\"/>"
msgstr ""
