msgstr ", <xref linkend=\"ST_NPoints\"/>, <xref linkend=\"ST_NumPoints\"/>"

#. Tag: refname
#: reference_editor.xml:936
#, no-c-format
msgid "ST_Reverse"
msgstr "ST_Reverse"

#. Tag: refpurpose
#: reference_editor.xml:937
#, no-c-format
msgid "Return the geometry with vertex order reversed."
