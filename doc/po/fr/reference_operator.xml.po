"(3 rows)"
msgstr ""
"SELECT tbl1.column1, tbl2.column1, tbl1.column2 &lt;&lt;| tbl2.column2 AS "
"below\n"
"FROM\n"
"  ( VALUES\n"
"        (1, 'LINESTRING (0 0, 4 3)'::geometry)) AS tbl1,\n"
"  ( VALUES\n"
"        (2, 'LINESTRING (1 4, 1 7)'::geometry),\n"
"        (3, 'LINESTRING (6 1, 6 5)'::geometry),\n"
