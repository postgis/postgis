"    ST_SameAlignment(not_aligned) AS not_aligned,\n"
"    ST_SameAlignment(aligned) AS aligned\n"
"FROM baz\n"
"\n"
" rast | not_aligned | aligned\n"
"------+-------------+---------\n"
" t    | f           | t"
