"        WHERE ST_Intersects(rast,\n"
"            ST_GeomFromText('POLYGON((224486 892151,224486 892200,224706 "
"892200,224706 892151,224486 892151))',26986)\n"
"             ) ;\n"
"\n"
" rid | count\n"
"-----+-------\n"
"   1 |    56\n"
"   2 |    95\n"
"  14 |    37\n"
"  15 |    64"
