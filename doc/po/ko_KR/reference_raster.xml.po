#, no-c-format
msgid ""
"SELECT\n"
"        *\n"
"FROM ST_BandMetadata(\n"
"        ST_AddBand(\n"
"                ST_MakeEmptyRaster(10, 10, 0, 0, 1, -1, 0, 0, 0),\n"
