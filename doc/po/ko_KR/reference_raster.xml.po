"    1 |   252 |   253 |     2 |    0.08\n"
"    1 |   253 |   254 |    18 |    0.72\n"
"    2 |    78 | 113.2 |    11 |    0.44\n"
"    2 | 113.2 | 148.4 |     4 |    0.16\n"
"    2 | 148.4 | 183.6 |     4 |    0.16\n"
"    2 | 183.6 | 218.8 |     1 |    0.04\n"
"    2 | 218.8 |   254 |     5 |     0.2\n"
"    3 |    62 | 100.4 |    11 |    0.44\n"
"    3 | 100.4 | 138.8 |     5 |     0.2\n"
"    3 | 138.8 | 177.2 |     4 |    0.16\n"
"    3 | 177.2 | 215.6 |     1 |    0.04\n"
"    3 | 215.6 |   254 |     4 |    0.16"
msgstr ""
"SELECT band, (stats).*\n"
