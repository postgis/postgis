"FROM foo t1\n"
"CROSS JOIN bar t2\n"
"CROSS JOIN bar t3\n"
"WHERE t2.gid = 1\n"
"        AND t3.gid = 2\n"
"ORDER BY t1.rid, t2.gid, t3.gid;\n"
"\n"
" rid | gid | gid |                                                    "
"st_dumpvalues\n"
"-----+-----+-----"
"+---------------------------------------------------------------------------------------------------------------------\n"
"   1 |   1 |   2 | (1,\"{{NULL,NULL,NULL,NULL,NULL},{NULL,2,2,2,NULL},"
"{NULL,2,2,2,NULL},{NULL,2,2,2,NULL},{NULL,NULL,NULL,NULL,NULL}}\")\n"
