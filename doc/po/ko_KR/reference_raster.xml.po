"    FROM dummy_rast CROSS JOIN generate_series(1,3) As band\n"
"     WHERE rid=2) As foo;\n"
"\n"
" band |  min  |  max  | count | percent\n"
"------+-------+-------+-------+---------\n"
"    1 |   249 |   250 |     2 |    0.08\n"
"    1 |   250 |   251 |     2 |    0.08\n"
"    1 |   251 |   252 |     1 |    0.04\n"
"    1 |   252 |   253 |     2 |    0.08\n"
"    1 |   253 |   254 |    18 |    0.72\n"
"    2 |    78 | 113.2 |    11 |    0.44\n"
"    2 | 113.2 | 148.4 |     4 |    0.16\n"
