"text, 1, 0\n"
"                                                                ),\n"
"                                                                1, 1, 0\n"
"                                                        ),\n"
"                                                        2, 3, 0\n"
"                                                ),\n"
"                                                3, 5, 0\n"
"                                        ),\n"
"                                        4, 2, 0\n"
"                                ),\n"
"                                5, 4, 255\n"
"                        )\n"
"                , 1, ARRAY[1, 255]) AS pixels\n"
") AS foo\n"
"\n"
" val | x | y\n"
"-----+---+---\n"
"   1 | 1 | 2\n"
"   1 | 1 | 3\n"
"   1 | 1 | 4\n"
"   1 | 1 | 5\n"
"   1 | 2 | 1\n"
"   1 | 2 | 2\n"
"   1 | 2 | 4\n"
"   1 | 2 | 5\n"
"   1 | 3 | 1\n"
"   1 | 3 | 2\n"
"   1 | 3 | 3\n"
"   1 | 3 | 4\n"
"   1 | 4 | 1\n"
"   1 | 4 | 3\n"
"   1 | 4 | 4\n"
"   1 | 4 | 5\n"
"   1 | 5 | 1\n"
"   1 | 5 | 2\n"
"   1 | 5 | 3\n"
" 255 | 5 | 4\n"
"   1 | 5 | 5"
msgstr ""
"SELECT\n"
"        (pixels).*\n"
"FROM (\n"
"        SELECT\n"
"                ST_PixelOfValue(\n"
"                        ST_SetValue(\n"
"                                ST_SetValue(\n"
"                                        ST_SetValue(\n"
"                                                ST_SetValue(\n"
"                                                        ST_SetValue(\n"
