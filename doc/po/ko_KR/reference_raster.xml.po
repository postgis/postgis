")\n"
"SELECT\n"
"        rid, gid, ST_DumpValues(ST_SetValue(rast, 1, geom, gid))\n"
"FROM foo t1\n"
"CROSS JOIN bar t2\n"
"ORDER BY rid, gid;\n"
"\n"
" rid | gid |                                                                "
"st_dumpvalues\n"
"-----+-----"
