"    2 | 2112500 | 174571 |  81.448503668639 | 44.2252623171821 |   0 | 255\n"
"    3 | 2112500 | 144364 | 74.6765884023669 | 44.2014869384578 |   0 | 255"
msgstr ""
"-- 각 밴드에 대한 통계 --\n"
"SELECT band, (stats).*\n"
"FROM (SELECT band, ST_SummaryStats('o_4_boston','rast', band) As stats\n"
