"+ - + - + - +          + - + - + - +\n"
"*/\n"
"SELECT\n"
"        (poly).x,\n"
"        (poly).y,\n"
"        (poly).val\n"
"FROM (\n"
"SELECT\n"
"        ST_PixelAsPolygons(\n"
"                ST_SetValues(\n"
"                        ST_AddBand(\n"
