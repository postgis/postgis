"\n"
" val | x | y\n"
"-----+---+---\n"
"   1 | 1 | 2\n"
"   1 | 1 | 3\n"
"   1 | 1 | 4\n"
"   1 | 1 | 5\n"
"   1 | 2 | 1\n"
"   1 | 2 | 2\n"
"   1 | 2 | 4\n"
"   1 | 2 | 5\n"
"   1 | 3 | 1\n"
"   1 | 3 | 2\n"
"   1 | 3 | 3\n"
"   1 | 3 | 4\n"
"   1 | 4 | 1\n"
"   1 | 4 | 3\n"
"   1 | 4 | 4\n"
"   1 | 4 | 5\n"
