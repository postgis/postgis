"    returning id, column1, column2, column3, geom\n"
")\n"
"insert into polygons_table (fid, column1, column2, column3, geom)\n"
"    select\n"
"        fid, column1, column2, column3,\n"
"        ST_Subdivide(geom, 255) as geom\n"
