#: reference_measure.xml:3549
#, no-c-format
msgid ""
", <xref linkend=\"DE-9IM\"/>, <xref linkend=\"ST_Disjoint\"/>, <xref linkend="
"\"ST_Intersects\"/>, <xref linkend=\"ST_Touches\"/>"
msgstr ""
