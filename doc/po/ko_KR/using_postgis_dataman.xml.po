"COLUMNNAME DBFFIELD1\n"
"AVERYLONGCOLUMNNAME DBFFIELD2"

#. Tag: term
#: using_postgis_dataman.xml:1254
#, no-c-format
msgid "<term>-S</term>"
msgstr "<term>-S</term>"

