"\n"
" st_box2dfromgeohash\n"
"----------------------\n"
" BOX(-180 -90,180 90)\n"
"\n"
" SELECT ST_Box2dFromGeoHash('9qqj7nmxncgyy4d0dbxqz0', 10);\n"
