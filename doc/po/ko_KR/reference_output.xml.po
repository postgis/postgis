"                        <subfield code=\"d\">W004.5792</subfield>\n"
"                        <subfield code=\"e\">W004.5466</subfield>\n"
"                        <subfield code=\"f\">N054.1970</subfield>\n"
"                        <subfield code=\"g\">N054.1817</subfield>\n"
"                    </datafield>\n"
"                </record>\n"
"\n"
"            ]]>"
