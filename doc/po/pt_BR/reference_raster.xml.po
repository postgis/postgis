msgstr "Disponibilidade: 2.2.0: Habilidade de adicionar máscara"

#. Tag: para
#: reference_raster.xml:6826 reference_raster.xml:6848
#, no-c-format
msgid "One raster, one band"
msgstr "Um raster, uma banda"

#. Tag: programlisting
#: reference_raster.xml:6827
#, no-c-format
msgid ""
"WITH foo AS (\n"
"        SELECT 1 AS rid, ST_AddBand(ST_MakeEmptyRaster(2, 2, 0, 0, 1, -1, 0, "
"0, 0), 1, '16BUI', 1, 0) AS rast\n"
")\n"
"SELECT\n"
"        ST_MapAlgebra(\n"
"                ARRAY[ROW(rast, 1)]::rastbandarg[],\n"
"                'sample_callbackfunc(double precision[], int[], text[])'::"
"regprocedure\n"
"        ) AS rast\n"
"FROM foo"
msgstr ""
"WITH foo AS (\n"
"        SELECT 1 AS rid, ST_AddBand(ST_MakeEmptyRaster(2, 2, 0, 0, 1, -1, 0, "
"0, 0), 1, '16BUI', 1, 0) AS rast\n"
