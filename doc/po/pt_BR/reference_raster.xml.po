"                'sample_callbackfunc(double precision[], int[], text[])'::"
"regprocedure\n"
"        ) AS rast\n"
"FROM foo t1\n"
"CROSS JOIN foo t2\n"
"WHERE t1.rid = 1\n"
"        AND t2.rid = 2"
msgstr ""
