"0),\n"
"                                1, '8BUI', 1, 0\n"
"                        ),\n"
"                        1, 2, 2, ARRAY[[9, 9], [9, 9]]::double precision[]"
"[]\n"
"                )\n"
"        ) AS poly\n"
") foo\n"
"ORDER BY 1, 2;\n"
"\n"
" x | y | val\n"
"---+---+-----\n"
" 1 | 1 |   1\n"
" 1 | 2 |   1\n"
" 1 | 3 |   1\n"
" 2 | 1 |   1\n"
" 2 | 2 |   9\n"
" 2 | 3 |   9\n"
" 3 | 1 |   1\n"
" 3 | 2 |   9\n"
" 3 | 3 |   9"

