"skewy | srid | numbands\n"
" ----+------------+------------+-------+--------+--------+-----------+-------"
"+-------+------+-------\n"
"   1 |        0.5 |        0.5 |    10 |     20 |      2 |      3 "
"|                0 |     0 |    0 |        0\n"
"   2 | 3427927.75 |    5793244 |     5 |      5 |   0.05 |  -0.05 "
"|                0 |     0 |    0 |        3"
msgstr ""
"SELECT rid, (foo.md).*\n"
" FROM (SELECT rid, ST_MetaData(rast) As md\n"
"FROM dummy_rast) As foo;\n"
"\n"
