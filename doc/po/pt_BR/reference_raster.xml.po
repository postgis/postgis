" 1 | 3 |   1\n"
" 2 | 1 |   1\n"
" 2 | 2 |   9\n"
" 2 | 3 |   9\n"
" 3 | 1 |   1\n"
" 3 | 2 |   9\n"
" 3 | 3 |   9"
