"                                [1, 1, 0, 1, 1]\n"
"                        ]::double precision[],\n"
"                        1\n"
"                ) AS rast\n"
") AS foo\n"
"\n"
"         st_neighborhood\n"
"---------------------------------\n"
" {{NULL,1,1},{1,1,NULL},{1,1,1}}"

