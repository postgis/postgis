"cd address_standardizer\n"
"make clean\n"
"make\n"
"make install\n"
"make installcheck\n"
"\n"
"cd ..\n"
"cd postgis_tiger_geocoder\n"
"make clean\n"
"make\n"
"make install\n"
"make installcheck"
msgstr ""
"cd extensions\n"
"cd postgis\n"
"make clean\n"
"make\n"
"make install\n"
"cd ..\n"
"cd postgis_topology\n"
"make clean\n"
