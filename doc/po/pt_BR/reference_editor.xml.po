"SELECT ST_AsEWKT(ST_SnapToGrid(ST_GeomFromEWKT('LINESTRING(-1.1115678 2.123 "
"3 2.3456,\n"
"                4.111111 3.2374897 3.1234 1.1111)'),\n"
"           0.01)      );\n"
"                                                st_asewkt\n"
"---------------------------------------------------------\n"
" LINESTRING(-1.11 2.12 3 2.3456,4.11 3.24 3.1234 1.1111)"
