#~ "and therefore does not overlap the line\n"
#~ "                        nor crosses\n"
#~ "\n"
#~ "SELECT ST_Overlaps(a,b) As a_overlap_b,\n"
#~ "        ST_Crosses(a,b) As a_crosses_b,\n"
#~ "                ST_Intersects(a, b) As a_intersects_b, ST_Contains(b,a) "
