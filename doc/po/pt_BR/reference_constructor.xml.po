"                        FROM waterlines w\n"
"                        WHERE ST_Within(w.the_geom, p.the_geom)\n"
"                        AND ST_IsClosed(w.the_geom))\n"
"                THEN\n"
"                ST_MakePolygon(ST_LineMerge(ST_Boundary(p.the_geom)),\n"
"                        ARRAY(SELECT w.the_geom\n"
"                                FROM waterlines w\n"
"                                WHERE ST_Within(w.the_geom, p.the_geom)\n"
"                                AND ST_IsClosed(w.the_geom)))\n"
"                ELSE p.the_geom END As the_geom\n"
"        FROM\n"
