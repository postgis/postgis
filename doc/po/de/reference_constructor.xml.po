#~ "\n"
#~ "SELECT\n"
#~ "  ST_AsText(\n"
#~ "        ST_GeomFromWKB(\n"
#~ "          ST_AsEWKB('POINT(2 5)'::geometry)\n"
#~ "        )\n"
#~ "  );\n"
#~ " st_astext\n"
#~ "------------\n"
#~ " POINT(2 5)\n"
#~ "(1 row)"

