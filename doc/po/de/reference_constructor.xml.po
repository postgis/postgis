#: reference_constructor.xml:689
#, no-c-format
msgid ""
"SELECT admin1.gid, hex.geom\n"
"FROM\n"
"    admin1\n"
"    CROSS JOIN\n"
"    ST_HexagonGrid(100000, admin1.geom) AS hex\n"
"WHERE\n"
"    adm0_a3 = 'USA'\n"
"    AND\n"
"    ST_Intersects(admin1.geom, hex.geom)"
