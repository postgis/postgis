"            )\n"
"ORDER BY value, quantile,rid\n"
";\n"
"\n"
"\n"
" rid | quantile | value\n"
"-----+----------+-------\n"
"   1 |        0 |     0\n"
"   2 |        0 |     0\n"
"  14 |        0 |     1\n"
"  15 |        0 |     2\n"
"  14 |     0.25 |    37\n"
"   1 |     0.25 |    42\n"
"  15 |     0.25 |    47\n"
"   2 |     0.25 |    50\n"
"  14 |      0.5 |    56\n"
"   1 |      0.5 |    64\n"
"  15 |      0.5 |    66\n"
"   2 |      0.5 |    77\n"
"  14 |     0.75 |    81\n"
"  15 |     0.75 |    87\n"
