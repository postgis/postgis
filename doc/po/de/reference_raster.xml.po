#: reference_raster.xml:2914
#, no-c-format
msgid ""
"SELECT\n"
"    rid,\n"
"    (ST_WorldToRasterCoord(rast,3427927.8,20.5)).*,\n"
"    (ST_WorldToRasterCoord(rast,ST_GeomFromText('POINT(3427927.8 20.5)',"
"ST_SRID(rast)))).*\n"
"FROM dummy_rast;\n"
"\n"
" rid | columnx |   rowy    | columnx |   rowy\n"
"-----+---------+-----------+---------+-----------\n"
