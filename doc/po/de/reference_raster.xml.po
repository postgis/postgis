"SELECT r1.rid, r2.rid, ST_Covers(r1.rast, 1, r2.rast, 1) FROM dummy_rast r1 "
"CROSS JOIN dummy_rast r2 WHERE r1.rid = 2;\n"
"\n"
" rid | rid | st_covers\n"
"-----+-----+-----------\n"
"   2 |   1 | f\n"
"   2 |   2 | t"
msgstr ""
