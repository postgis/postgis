"------+-------+-------+-------+---------\n"
"    1 |   249 |   250 |     2 |    0.08\n"
"    1 |   250 |   251 |     2 |    0.08\n"
"    1 |   251 |   252 |     1 |    0.04\n"
"    1 |   252 |   253 |     2 |    0.08\n"
"    1 |   253 |   254 |    18 |    0.72\n"
"    2 |    78 | 113.2 |    11 |    0.44\n"
"    2 | 113.2 | 148.4 |     4 |    0.16\n"
"    2 | 148.4 | 183.6 |     4 |    0.16\n"
"    2 | 183.6 | 218.8 |     1 |    0.04\n"
