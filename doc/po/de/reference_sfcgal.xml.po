msgstr "Vereinigung von geom1 und geom2"

#. Tag: para
#: reference_sfcgal.xml:679
#, no-c-format
msgid ""
", <xref linkend=\"ST_AsX3D\"/>, <xref linkend=\"ST_3DIntersection\"/> <xref "
"linkend=\"ST_3DDifference\"/>"
