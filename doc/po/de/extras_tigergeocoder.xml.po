#: extras_tigergeocoder.xml:538
#, no-c-format
msgid ""
"<![CDATA[psql -U postgres -h localhost -d gistest -A -t \\\n"
" -c \"SELECT Loader_Generate_Script(ARRAY['MA'], 'gistest')\" > /gisdata/"
"data_load.sh;]]>"
msgstr ""
"<![CDATA[psql -U postgres -h localhost -d gistest -A -t \\\n"
" -c \"SELECT Loader_Generate_Script(ARRAY['MA'], 'gistest')\" > /gisdata/"
