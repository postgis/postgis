"btree(countyfp);\n"
"CREATE INDEX idx_tiger_cousub_countyfp ON tiger.cousub USING "
"btree(countyfp);\n"
"CREATE INDEX idx_tiger_edges_tfidr ON tiger.edges USING btree(tfidr);\n"
