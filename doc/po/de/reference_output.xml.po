msgid "To build FeatureCollection:"
msgstr ""

#. Tag: programlisting
#: reference_output.xml:524
#, no-c-format
msgid ""
"select json_build_object(\n"
"    'type', 'FeatureCollection',\n"
"    'features', json_agg(ST_AsGeoJSON(t.*)::json)\n"
"    )\n"
"from ( values (1, 'one', 'POINT(1 1)'::geometry),\n"
"              (2, 'two', 'POINT(2 2)'),\n"
"              (3, 'three', 'POINT(3 3)')\n"
"     ) as t(id, name, geom);"
msgstr ""

