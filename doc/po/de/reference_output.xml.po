#. Tag: programlisting
#: reference_output.xml:1173
#, no-c-format
msgid ""
"SELECT ST_AsX3D(\n"
"ST_Translate(\n"
"    ST_Force_3d(\n"
"        ST_Buffer(ST_Point(10,10),5, 'quad_segs=2')), 0,0,\n"
"    3)\n"
"  ,6) As x3dfrag;\n"
"\n"
"x3dfrag\n"
"--------\n"
"<![CDATA[<IndexedFaceSet coordIndex=\"0 1 2 3 4 5 6 7\">\n"
"    <Coordinate point=\"15 10 3 13.535534 6.464466 3 10 5 3 6.464466 "
"6.464466 3 5 10 3 6.464466 13.535534 3 10 15 3 13.535534 13.535534 3 \" />\n"
"</IndexedFaceSet>]]>"
msgstr ""
"SELECT ST_AsX3D(\n"
"ST_Translate(\n"
"    ST_Force_3d(\n"
"        ST_Buffer(ST_Point(10,10),5, 'quad_segs=2')), 0,0,\n"
"    3)\n"
"  ,6) As x3dfrag;\n"
"\n"
"x3dfrag\n"
"--------\n"
"<![CDATA[<IndexedFaceSet coordIndex=\"0 1 2 3 4 5 6 7\">\n"
"    <Coordinate point=\"15 10 3 13.535534 6.464466 3 10 5 3 6.464466 "
"6.464466 3 5 10 3 6.464466 13.535534 3 10 15 3 13.535534 13.535534 3 \" />\n"
"</IndexedFaceSet>]]>"

#. Tag: title
