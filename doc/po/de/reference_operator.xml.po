"        (1, 'LINESTRING (1 4, 1 7)'::geometry)) AS tbl1,\n"
"  ( VALUES\n"
"        (2, 'LINESTRING (0 0, 4 2)'::geometry),\n"
"        (3, 'LINESTRING (6 1, 6 5)'::geometry),\n"
"        (4, 'LINESTRING (2 3, 5 6)'::geometry)) AS tbl2;\n"
"\n"
" column1 | column1 | above\n"
"---------+---------+-------\n"
"           1 |       2 | t\n"
"           1 |       3 | f\n"
"           1 |       4 | f\n"
"(3 rows)"
msgstr ""
