"                SUM(ST_MemSize(the_geom))*100 As numeric(10,2)) As perbos\n"
"FROM towns;\n"
"\n"
"totgeomsum        bossum        perbos\n"
"----------        ------        ------\n"
"1522 kB                30 kB        1.99\n"
"\n"
"\n"
