"        FROM polys As b\n"
"INNER JOIN compounds As l\n"
"ON (p.the_geom &amp;&amp; b.the_geom\n"
"AND ST_Relate(l.the_geom, b.the_geom,'T********'));\n"
"\n"
"SELECT ST_Relate(ST_GeometryFromText('POINT(1 2)'), ST_Buffer(ST_GeometryFromText('POINT(1 2)'),2));\n"
