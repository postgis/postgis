#: reference_raster.xml:2368
#, no-c-format
msgid ""
", <xref linkend=\"RT_ST_RasterToWorldCoordY\"/>, <xref linkend="
"\"RT_ST_SetSkew\"/>, <xref linkend=\"RT_ST_SkewX\"/>"
msgstr ""

