"   2 |   1 |      98 |         14 |                        14\n"
"   3 |   1 |     122 |            |                         0\n"
"   1 |   2 |      96 |         14 |                        14\n"
"   2 |   2 |     118 |            |                         0\n"
"   3 |   2 |     180 |            |                         0\n"
"   1 |   3 |      99 |         15 |                        15\n"
"   2 |   3 |     112 |            |                         0\n"
"   3 |   3 |     169 |            |                         0"
msgstr ""

