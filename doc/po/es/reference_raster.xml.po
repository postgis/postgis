"regprocedure\n"
"        ) AS rast\n"
"FROM foo"
msgstr ""

#. Tag: programlisting
#: reference_raster.xml:6849
#, no-c-format
msgid ""
"WITH foo AS (\n"
"        SELECT 1 AS rid, "
"ST_AddBand(ST_AddBand(ST_AddBand(ST_MakeEmptyRaster(2, 2, 0, 0, 1, -1, 0, 0, "
"0), 1, '16BUI', 1, 0), 2, '8BUI', 10, 0), 3, '32BUI', 100, 0) AS rast\n"
")\n"
"SELECT\n"
"        ST_MapAlgebra(\n"
"                rast, 2,\n"
