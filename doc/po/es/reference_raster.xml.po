"        <Value>NONE</Value>\n"
"        <Value>LZW</Value>\n"
"        <Value>PACKBITS</Value>\n"
"        <Value>JPEG</Value>\n"
"        <Value>CCITTRLE</Value>\n"
"        <Value>CCITTFAX3</Value>\n"
"        <Value>CCITTFAX4</Value>\n"
