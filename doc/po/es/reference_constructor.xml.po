"  ST_LineStringFromWKB(\n"
"        ST_AsBinary(ST_GeomFromText('LINESTRING(1 2, 3 4)'))\n"
"  ) AS aline,\n"
"  ST_LinestringFromWKB(\n"
"        ST_AsBinary(ST_GeomFromText('POINT(1 2)'))\n"
"  ) IS NULL AS null_return;\n"
