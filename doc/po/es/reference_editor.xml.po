msgid ""
"SELECT ST_AsText(ST_QuantizeCoordinates('POINT (100.123456 0)'::geometry, "
"4));\n"
"st_astext\n"
"-------------------------\n"
"POINT(100.123455047607 0)"
msgstr ""

