msgstr ""

#. Tag: para
#: reference_measure.xml:3529
#, no-c-format
msgid "not in OGC spec, but implied. see s2.1.13.2"
msgstr ""

#. Tag: para
#: reference_measure.xml:3534
#, no-c-format
msgid "&sqlmm_compliant; SQL-MM 3: 5.1.25"
msgstr ""

#. Tag: para
#: reference_measure.xml:3535
