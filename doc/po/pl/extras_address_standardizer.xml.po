#, no-c-format
msgid "Single Addresss"
msgstr ""

#. Tag: programlisting
#: extras_address_standardizer.xml:601
#, no-c-format
msgid ""
"SELECT num, street, city, zip, zipplus\n"
"        FROM parse_address('1 Devonshire Place, Boston, MA 02109-1234') AS a;"
msgstr ""

