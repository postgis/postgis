"        FROM\n"
"        (SELECT ST_LocateAlong(\n"
"                        ST_GeomFromText('MULTILINESTRINGM((1 2 3, 3 4 2, 9 4 "
"3),\n"
"        (1 2 3, 5 4 5))'),3) As the_geom) As foo;\n"
"\n"
"   st_asewkt\n"
"---------------\n"
" POINTM(1 2 3)\n"
" POINTM(9 4 3)\n"
" POINTM(1 2 3)"
