#: reference_editor.xml:10
#, no-c-format
msgid "Geometry Editors"
msgstr "ジオメトリエディタ"

#. Tag: refname
#: reference_editor.xml:14
#, no-c-format
msgid "ST_AddPoint"
msgstr "ST_AddPoint"

#. Tag: refpurpose
#: reference_editor.xml:15
#, no-c-format
msgid "Add a point to a LineString."
msgstr "ラインストリングにポイントを追加します。"

#. Tag: funcprototype
#: reference_editor.xml:19
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_AddPoint</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>linestring</parameter></paramdef> "
"<paramdef><type>geometry</type> <parameter>point</parameter></paramdef>"
msgstr ""
"<funcdef>geometry <function>ST_AddPoint</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>linestring</parameter></paramdef> "
"<paramdef><type>geometry</type> <parameter>point</parameter></paramdef>"

#. Tag: funcprototype
#: reference_editor.xml:26
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_AddPoint</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>linestring</parameter></paramdef> "
"<paramdef><type>geometry</type> <parameter>point</parameter></paramdef> "
"<paramdef><type>integer</type> <parameter>position = -1</parameter></"
"paramdef>"
msgstr ""
"<funcdef>geometry <function>ST_AddPoint</function></funcdef> "
"<paramdef><type>geometry</type> <parameter>linestring</parameter></paramdef> "
"<paramdef><type>geometry</type> <parameter>point</parameter></paramdef> "
"<paramdef><type>integer</type> <parameter>position = -1</parameter></"
"paramdef>"

