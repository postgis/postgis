#, no-c-format
msgid ""
"SELECT ST_EstimatedExtent('ny', 'edges', 'geom');\n"
"--result--\n"
"BOX(-8877653 4912316,-8010225.5 5589284)\n"
"\n"
"SELECT ST_EstimatedExtent('feature_poly', 'geom');\n"
"--result--\n"
"BOX(-124.659652709961 24.6830825805664,-67.7798080444336 49.0012092590332)"
msgstr ""
"SELECT ST_EstimatedExtent('ny', 'edges', 'geom');\n"
"-- 結果 --\n"
"BOX(-8877653 4912316,-8010225.5 5589284)\n"
"\n"
"SELECT ST_EstimatedExtent('feature_poly', 'geom');\n"
"-- 結果 --\n"
"BOX(-124.659652709961 24.6830825805664,-67.7798080444336 49.0012092590332)"

