"ST_Translate(ST_ExteriorRing(ST_Buffer(ST_MakePoint(x1*10,y1), z1)),y1*1, "
"z1*2) As line\n"
"        FROM generate_series(-3,6) x1\n"
"        CROSS JOIN generate_series(2,5) y1\n"
"        CROSS JOIN generate_series(1,10) z1\n"
