#~ "    ST_GeomFromText('MULTILINESTRING((10 10, 190 190), (15 15, 30 30, 100 "
#~ "90))') As mline,\n"
#~ "    ST_Point(30,30) As pt) As foo;\n"
#~ "\n"
#~ "wktcut\n"
#~ "------\n"
