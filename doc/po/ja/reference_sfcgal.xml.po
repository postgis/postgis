#~ "49.5,10 10 3.40282346638529e+38)"
#~ msgstr ""
#~ "select ST_AsText(ST_SetEffectiveArea(geom)) all_pts, "
#~ "ST_AsText(ST_SetEffectiveArea(geom,30) ) thrshld_30\n"
#~ "FROM (SELECT  'LINESTRING(5 2, 3 8, 6 20, 7 25, 10 10)'::geometry geom) "
