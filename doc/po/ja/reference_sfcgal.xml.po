"SELECT ST_MinkowskiSum(mp, poly)\n"
"FROM (SELECT 'MULTIPOINT(25 50,70 25)'::geometry As mp,\n"
"   'POLYGON((130 150, 20 40, 50 60, 125 100, 130 150))'::geometry As poly\n"
"    ) As foo\n"
