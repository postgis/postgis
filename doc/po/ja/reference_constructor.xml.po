msgstr ""
"SELECT ST_AsText(ST_MakeEnvelope(10, 10, 11, 11, 4326));\n"
"\n"
"st_asewkt\n"
"-----------\n"
"POLYGON((10 10, 10 11, 11 11, 11 10, 10 10))"

