#~ "\\000\\\\000\\\\037\\\\205\\\\353Q\\\\270~\\\\\\\\\\\\300\\\\323Mb\\\\020X"
#~ "\\\\231C@\\\\020X9\\\\264\\\\310~\\\\\\\\\\\\300)\\\\\\\\\\\\217\\\\302\\"
#~ "\\365\\\\230C@',4326)\n"
#~ ");\n"
#~ "                                          st_asewkt\n"
#~ "------------------------------------------------------\n"
#~ " SRID=4326;LINESTRING(-113.98 39.198,-113.981 39.195)\n"
#~ "(1 row)\n"
#~ "\n"
#~ "SELECT\n"
#~ "  ST_AsText(\n"
#~ "        ST_GeomFromWKB(\n"
#~ "          ST_AsEWKB('POINT(2 5)'::geometry)\n"
