#, no-c-format
msgid ""
"SELECT ST_LineCrossingDirection(foo.line1, foo.line2) As l1_cross_l2 ,\n"
"    ST_LineCrossingDirection(foo.line2, foo.line1) As l2_cross_l1\n"
