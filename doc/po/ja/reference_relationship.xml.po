"SELECT ST_Relate( 'POINT(1 2)',\n"
"                  ST_Buffer( 'POINT(1 2)', 2));\n"
"st_relate\n"
"-----------\n"
"0FFFFF212\n"
"\n"
"SELECT ST_Relate( 'LINESTRING(1 2, 3 4)',\n"
"                  'LINESTRING(5 6, 7 8)' );\n"
"st_relate\n"
"-----------\n"
"FF1FF0102"

