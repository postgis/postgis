"SELECT topology.AddTopoGeometryColumn('ri_topo', 'ri', 'roads', 'topo', "
"'LINE');"
msgstr ""
"CREATE SCHEMA ri;\n"
"CREATE TABLE ri.roads(gid serial PRIMARY KEY, road_name text);\n"
"SELECT topology.AddTopoGeometryColumn('ri_topo', 'ri', 'roads', 'topo', "
"'LINE');"

