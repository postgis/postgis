"-----+--------------------------------------------------------------------\n"
"   1 | POLYGON((0 0,20 0,20 60,0 60,0 0))\n"
"   2 | POLYGON((3427927 5793243,3427928 5793243,\n"
"                3427928 5793244,3427927 5793244, 3427927 5793243))"
msgstr ""
"SELECT rid, ST_AsText(ST_Envelope(rast)) As envgeomwkt\n"
"FROM dummy_rast;\n"
"\n"
" rid |                                         envgeomwkt\n"
"-----+--------------------------------------------------------------------\n"
