"                                        )\n"
"                                        , 1, 1, 1, -10\n"
"                                )\n"
"                                , 1, 5, 4, 0\n"
"                        )\n"
"                        , 1, 5, 5, 3.14159\n"
"                ) AS rast\n"
"        ) AS rast\n"
"        FULL JOIN (\n"
"                SELECT generate_series(1, 10) AS id\n"
"        ) AS id\n"
"                ON 1 = 1\n"
")\n"
"SELECT\n"
"        (stats).count,\n"
"        round((stats).sum::numeric, 3),\n"
"        round((stats).mean::numeric, 3),\n"
"        round((stats).stddev::numeric, 3),\n"
"        round((stats).min::numeric, 3),\n"
"        round((stats).max::numeric, 3)\n"
"FROM (\n"
"        SELECT\n"
