"SELECT ST_AsText(ST_SetSRID(\n"
"        ST_Point(ST_UpperLeftX(rast) + ST_ScaleX(rast)*x,\n"
"                ST_UpperLeftY(rast) + ST_ScaleY(rast)*y),\n"
"                ST_SRID(rast))) As uplpt\n"
"    , ST_Value(rast, 1, x, y) As b1val,\n"
