"ST_AddBand(\n"
"  ST_MakeEmptyRaster(1000, 1000, 0, 0, 2),\n"
"  1, '8BSI'::text, -129, NULL\n"
") r1;\n"
"\n"
"CREATE TABLE res2 AS SELECT\n"
"ST_AddBand(\n"
"  ST_MakeEmptyRaster(500, 500, 0, 0, 4),\n"
"  1, '8BSI'::text, -129, NULL\n"
") r2;\n"
"\n"
"SELECT AddOverviewConstraints('res2', 'r2', 'res1', 'r1', 2);\n"
"\n"
