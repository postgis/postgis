#: reference_processing.xml:1934 reference_processing.xml:2053
#, no-c-format
msgid ""
"SELECT ST_VoronoiLines(geom, 30) As geom\n"
"FROM (SELECT 'MULTIPOINT (50 30, 60 30, 100 100,10 150, 110 120)'::geometry "
"As geom ) As g"
msgstr ""
"SELECT ST_VoronoiLines(geom, 30) As geom\n"
"FROM (SELECT 'MULTIPOINT (50 30, 60 30, 100 100,10 150, 110 120)'::geometry "
"As geom ) As g"

