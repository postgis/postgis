#. Tag: programlisting
#: reference_processing.xml:1054
#, no-c-format
msgid ""
"SELECT edge_id, (dp).path[1] As index, ST_AsText((dp).geom) As wktnode\n"
"FROM (SELECT 1 As edge_id\n"
"        , ST_DumpPoints(ST_GeomFromText('LINESTRING(1 2, 3 4, 10 10)')) AS "
"dp\n"
"     UNION ALL\n"
"     SELECT 2 As edge_id\n"
"        , ST_DumpPoints(ST_GeomFromText('LINESTRING(3 5, 5 6, 9 10)')) AS "
"dp\n"
"   ) As foo;\n"
" edge_id | index |    wktnode\n"
"---------+-------+--------------\n"
"       1 |     1 | POINT(1 2)\n"
"       1 |     2 | POINT(3 4)\n"
"       1 |     3 | POINT(10 10)\n"
"       2 |     1 | POINT(3 5)\n"
"       2 |     2 | POINT(5 6)\n"
"       2 |     3 | POINT(9 10)"
msgstr ""
"SELECT edge_id, (dp).path[1] As index, ST_AsText((dp).geom) As wktnode\n"
"FROM (SELECT 1 As edge_id\n"
"        , ST_DumpPoints(ST_GeomFromText('LINESTRING(1 2, 3 4, 10 10)')) AS "
"dp\n"
"     UNION ALL\n"
"     SELECT 2 As edge_id\n"
"        , ST_DumpPoints(ST_GeomFromText('LINESTRING(3 5, 5 6, 9 10)')) AS "
"dp\n"
"   ) As foo;\n"
" edge_id | index |    wktnode\n"
"---------+-------+--------------\n"
"       1 |     1 | POINT(1 2)\n"
"       1 |     2 | POINT(3 4)\n"
"       1 |     3 | POINT(10 10)\n"
"       2 |     1 | POINT(3 5)\n"
"       2 |     2 | POINT(5 6)\n"
"       2 |     3 | POINT(9 10)"

