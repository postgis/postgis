#: reference_processing.xml:1512
#, no-c-format
msgid ""
"SELECT d.disease_type,\n"
"        ST_MinimumBoundingCircle(ST_Collect(d.the_geom)) As the_geom\n"
"        FROM disease_obs As d\n"
"        GROUP BY d.disease_type;"
msgstr ""
"SELECT d.disease_type,\n"
"        ST_MinimumBoundingCircle(ST_Collect(d.the_geom)) As the_geom\n"
"        FROM disease_obs As d\n"
"        GROUP BY d.disease_type;"

