msgstr ""
"SELECT ST_AsText(ST_MinimumBoundingCircle(\n"
"        ST_Collect(\n"
"            ST_GeomFromText('LINESTRING(55 75,125 150)'),\n"
"                ST_Point(20, 80)), 8\n"
"                )) As wktmbc;\n"
"wktmbc\n"
"-----------\n"
"POLYGON((135.59714732062 115,134.384753327498 "
