"select ST_AsText(ST_ChaikinSmoothing(geom)) smoothed\n"
"FROM (SELECT  'POLYGON((0 0, 8 8, 0 16, 0 0))'::geometry geom) As foo;\n"
"┌───────────────────────────────────────────┐\n"
"│                 smoothed                  │\n"
"├───────────────────────────────────────────┤\n"
"│ POLYGON((2 2,6 6,6 10,2 14,0 12,0 4,2 2)) │\n"
"└───────────────────────────────────────────┘"
msgstr ""
"select ST_AsText(ST_ChaikinSmoothing(geom)) smoothed\n"
"FROM (SELECT  'POLYGON((0 0, 8 8, 0 16, 0 0))'::geometry geom) As foo;\n"
"┌───────────────────────────────────────────┐\n"
"│                 smoothed                  │\n"
"├───────────────────────────────────────────┤\n"
"│ POLYGON((2 2,6 6,6 10,2 14,0 12,0 4,2 2)) │\n"
"└───────────────────────────────────────────┘"

