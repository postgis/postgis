"--using polygons\n"
"SELECT ST_BuildArea(ST_Collect(smallc,bigc))\n"
"FROM (SELECT\n"
"    ST_Buffer(\n"
"      ST_GeomFromText('POINT(100 90)'), 25) As smallc,\n"
"    ST_Buffer(ST_GeomFromText('POINT(100 90)'), 50) As bigc) As foo;"
msgstr ""
"-- ポリゴンを使用\n"
"SELECT ST_BuildArea(ST_Collect(smallc,bigc))\n"
"FROM (SELECT\n"
"    ST_Buffer(\n"
"      ST_GeomFromText('POINT(100 90)'), 25) As smallc,\n"
"    ST_Buffer(ST_GeomFromText('POINT(100 90)'), 50) As bigc) As foo;"

