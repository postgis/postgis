"FROM\n"
"(SELECT ST_MakeLine(ST_MakePoint(1,2),\n"
"                ST_MakePoint(1,10)) As the_geom) as foo;\n"
"--result\n"
"                line         |     reverseline\n"
"---------------------+----------------------\n"
"LINESTRING(1 2,1 10) | LINESTRING(1 10,1 2)"
