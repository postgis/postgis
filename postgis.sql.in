CREATE TYPE SPHEROID (
	alignment = double,
	internallength = 65,
	input =  SPHEROID_in,
	output =  SPHEROID_out
);

CREATE TYPE BOX3D (
	alignment = double,
	internallength = 48,
	input = BOX3D_in,
	output = BOX3D_out
);

CREATE TYPE WKB (
	alignment = double,
	internallength = VARIABLE,
	input =  textin,
	output =  textout
);



create function geometry_in(opaque)
