   AS '@MODULE_FILENAME@'
   LANGUAGE 'c' with (isstrict);

CREATE TYPE GEOMETRY (
	alignment = double,
	internallength = VARIABLE,
	input =  geometry_in,
	output =  geometry_out,
	storage = main
);


