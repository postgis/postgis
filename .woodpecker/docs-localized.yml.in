  @TARGET@-@LANG@:
    image: *test_image
    commands:
      - make -C build-docs/doc/po/@LANG@ local-@TARGET@
    #group: @GROUP@
    when:
      path:
        include:
          # Localized assets
          - "doc/po/@LANG@/Makefile.in"
          - "doc/po/@LANG@/*.po"
          - "utils/check_localized_cheatsheets.sh"
